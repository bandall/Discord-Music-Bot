    "dotenv": "^16.0.3",
    "ffmpeg-static": "^4.4.1",
    "fs": "^0.0.1-security",
    "libsodium-wrappers": "^0.7.10",
    "nodemon": "^2.0.20",
    "path": "^0.12.7",
    "play-dl": "^1.9.6"
